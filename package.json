{
    "name": "eleventastic",
    "version": "1.0.0",
    "description": "A boilerplate for eleventy websites",
    "browserslist": [
        "defaults"
    ],
    "scripts": {
        "start": "npm-run-all -p dev:*",
        "build": "run-s clean build:* --print-label",
        "dev:eleventy": "cross-env ELEVENTY_ENV=development eleventy --serve",
        "build:eleventy": "cross-env ELEVENTY_ENV=production eleventy",
        "dev:css": "postcss src/assets/styles/main.css -o dist/assets/styles/main.css --verbose --watch",
        "build:css": "postcss src/assets/styles/main.css -o dist/assets/styles/main.css --verbose",
        "clean": "del-cli dist",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "http://github.com/maxboeck/eleventastic"
    },
    "author": "Max BÃ¶ck",
    "license": "MIT",
    "dependencies": {
        "@11ty/eleventy-img": "^0.8.3",
        "@11ty/eleventy-navigation": "^0.1.6",
        "@11ty/eleventy-plugin-rss": "^1.1.1",
        "@babel/core": "^7.13.14",
        "@babel/plugin-transform-runtime": "^7.13.10",
        "@babel/preset-env": "^7.13.12",
        "@tailwindcss/aspect-ratio": "^0.4.0",
        "@tailwindcss/forms": "^0.4.0",
        "@tailwindcss/typography": "^0.5.2",
        "babel-loader": "^8.2.2",
        "browser-sync": "^2.27.7",
        "clean-css": "^5.1.2",
        "concurrently": "^7.0.0",
        "critical": "^3.0.0",
        "cssesc": "^3.0.0",
        "cssnano": "^5.0.17",
        "del-cli": "^3.0.1",
        "eleventy-plugin-svg-sprite": "^1.2.1",
        "express": "^4.18.2",
        "focus-trap": "^6.3.0",
        "focus-visible": "^5.2.0",
        "html-minifier": "^4.0.0",
        "luxon": "^1.26.0",
        "markdown-it": "^12.0.4",
        "memfs": "^3.2.0",
        "postcss-cli": "^9.1.0",
        "sass": "^1.62.1",
        "sass-loader": "^13.3.0",
        "svg-sprite": "^1.5.0",
        "webpack": "^5.28.0"
    },
    "devDependencies": {
        "@11ty/eleventy": "^2.0.1",
        "autoprefixer": "^10.4.14",
        "cross-env": "^7.0.3",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.24",
        "postcss-cli": "^10.1.0",
        "tailwindcss": "^3.3.2"
    }
}
